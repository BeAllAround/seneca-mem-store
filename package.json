  "scripts": {
    "test": "lab -v -t 80 -r console -o stdout -r html -o test/coverage.html",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -t 80 -r html > coverage.html",
    "prettier": "prettier --write --no-semi --single-quote *.js lib/*.js test/*.js",
    "reset": "npm run clean && npm i && npm test",
    "clean": "rm -rf node_modules package-lock.json yarn.lock",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean && npm i --registry=http://registry.npmjs.org && npm run prettier && npm test && npm run repo-tag && npm publish --access public --registry=http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-mem-store"
  },
  "keywords": [
    "seneca",
    "plugin",
    "store",
    "mem",
    "memory"
  ],
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/lab": "^22.0.4",
    "coveralls": "^3.1.0",
    "prettier": "^2.0.5",
    "seneca": "^3.21.0",
    "seneca-entity": "^8.0.0",
    "seneca-store-test": "^1.5.1"
  },
  "files": [
    "README.md",
    "LICENSE",
    "mem-store.js",
    "lib/errors.js"
  ]
